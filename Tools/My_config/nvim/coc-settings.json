// vim:ft=jsonc
{
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.formatOnSaveFiletypes": [],
  // "coc.preferences.jumpCommand": "wincmd o | vsplit",
  "coc.preferences.messageLevel": "more",
  "coc.preferences.promptInput": false,
  "coc.preferences.promptWorkspaceEdit": true,
  "coc.preferences.rootPatterns": [
    ".git",
    "Cargo.toml",
    ".root",
    ".gitignore",
    "Makefile",
    ".tasks",
    "CMakeLists.txt"
  ],
  "coc.source.around.priority": 8,
  "coc.source.buffer.priority": 1,
  "coc.source.emoji.filetypes": ["markdown", "gitcommit"],
  "coc.source.emoji.priority": 3,
  "coc.source.emoji.shortcut": "E",
  "coc.source.file.priority": 10,
  "coc.source.tag.priority": 5,
  "cursors.cancelKey": "<esc>",
  "explorer.enableFloatinput": false,
  "explorer.file.autoReveal": true,
  "explorer.git.showIgnored": true,
  "explorer.git.icon.status.ignored": " ",
  "explorer.file.column.indent.indentLine": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.keyMappingMode": "none",
  "explorer.keyMappings.global": {
    "<C-k>": "nodePrev",
    "<M-k>": "indentPrev",
    "<C-j>": "nodeNext",
    "<M-j>": "indentNext",
    "*": "toggleSelection",
    "<tab>": "actionMenu",
    "h": "collapse",
    "l": ["expandable?", "expand", "open"],
    "J": ["toggleSelection", "normal:j"],
    "K": ["toggleSelection", "normal:k"],
    "gl": "expand:recursive",
    "gh": "collapse:recursive",
    "o": ["expanded?", "collapse", "expand"],
    "O": "systemExecute",
    "<cr>": "open",
    "<C-l>": "open:select:keep",
    "E": "openInVsplit",
    "t": "openInTab",
    "<bs>": "gotoParent",
    "<C-h>": "gotoParent",
    "y": "copyFilepath",
    "Y": "copyFilename",
    "C": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "P": "preview:content",
    "D": "delete",
    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",
    ".": "toggleHidden",
    "R": "refresh",
    "?": "help",
    "q": "quit",
    "X": "systemExecute",
    "gd": "listDrive",
    "f": "search",
    "F": "searchRecursive",
    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",
    "[[": "sourcePrev",
    "]]": "sourceNext",
    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",
    "ga": "gitStage",
    "gu": "gitUnstage",
    "gk": "gitPrev",
    "gj": "gitNext",
    "<": "gitStage",
    ">": "gitUnstage"
  },
  "explorer.openAction.select.filter": {
    "buftypes": ["terminal"],
    "filetypes": ["vista", "qf"],
    "floatingWindows": true
  },
  "explorer.file.cdCommand": "cd",
  "explorer.previewAction.onHover": false,
  "explorer.quitOnOpen": false,
  "explorer.trash.command": "rm -r %s",
  "explorer.width": 30,
  "git.addedSign.hlGroup": "GitAdd",
  "git.changeRemovedSign.hlGroup": "GitDeleteTopAndBottom",
  "git.changedSign.hlGroup": "GitModify",
  "git.enableGutters": true,
  "git.removedSign.hlGroup": "GitDeleteBottom",
  "git.semanticCommit.scope": false,
  "git.showCommitInFloating": true,
  "git.topRemovedSign.hlGroup": "GitDeleteTop",
  // "http.proxy": "socks5://127.0.0.1:1081",
  // "http.proxyStrictSSL": true,
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh", "zsh", "make"],
      "ignoredRootPaths": ["~"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim", ".git"],
      "filetypes": ["go"]
    }
  },
  "list.source.grep.defaultArgs": ["-smartcase", "-regex"],
  "list.indicator": ">",
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-j>": "do:next",
    "<C-k>": "do:previous",
    "<C-r>": "action:preview",
    "<C-t>": "do:toggle"
  },
  "list.normalMappings": {
    "<C-c>": "do:exit",
    "<C-j>": "do:next",
    "<C-k>": "do:previous",
    "<C-p>": "action:preview",
    "p": "action:preview",
    "<C-t>": "do:toggle"
  },
  "list.previewSplitRight": false,
  "session.directory": "~/.cache/nvim/session",
  "session.saveOnVimLeave": false,
  "signature.preferShownAbove": false,
  "snippets.extends": {
    "cpp": ["c"],
    "typescript": ["javascript"],
    "javascriptreact": ["javascript"],
    "typescriptreact": ["typescript"]
  },
  "snippets.priority": -10,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.completionItemKindLabels": {
    "class": "\uf0e8",
    "color": "\ue22b",
    "constant": "\uf8fe",
    "default": "\uf29c",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "event": "\ufacd",
    "field": "\uf9be",
    "file": "\uf723",
    "folder": "\uf114",
    "function": "\u0192",
    "interface": "\uf417",
    "keyword": "\uf1de",
    "method": "\uf09a",
    "module": "\uf40d",
    "operator": "\u03a8",
    "property": "\ue624",
    "reference": "\ufa46",
    "snippet": "\ue60b",
    "struct": "\ufb44",
    "text": "\ue612",
    "typeParameter": "\uf728",
    "unit": "\uf475",
    "value": "\uf89f",
    "variable": "\ue79b"
  },
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.detailMaxLength": 50,
  "suggest.enablePreselect": false,
  "suggest.labelMaxLength": 75,
  "suggest.languageSourcePriority": 99,
  "suggest.noselect": false,
  "suggest.numberSelect": false,
  "suggest.removeDuplicateItems": true,
  "python.pythonPath": "/Users/mikizhu/miniconda3/bin/python",
  "suggest.triggerAfterInsertEnter": false,
}
